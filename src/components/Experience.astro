---
import ExperienceItem from "./ExperienceItem.astro";
const EXPERIENCE = [
    {
        date: 'November 2023',
        title: 'Desarrollador web en VorticeSoft',
        description: 'Desarrollador de proyectos web en VorticeSoft',
        link: '#'
    },
    {
        date: 'Febreary 2023',
        title: 'Desarrollador web en Fundación Magtel',
        description: 'Desarrollador de página web de voluntariado en Fundación Magtel',
        link: '#'
    }
]
---

<ol class="relative border-s border-gray-200 dark:border-gray-700">
    {
        EXPERIENCE.map(experience => (
                <li class="mb-10 ms-4">
                    <ExperienceItem {...experience}/>
                </li>
        )
        )
    }
</ol>

